@model Client

<dl class="dl-horizontal" style="margin: 10px">
    <dt class="card-text">@Localizer["Firstname"]</dt>
    <dd class="card-text">@Model.Firstname</dd>
    
    <dt class="card-text">@Localizer["Lastname"]</dt>
    <dd class="card-text">@Model.Lastname</dd>
    
    <dt class="card-text">@Localizer["Phone"]</dt>
    <dd class="card-text">@Model.Phone</dd>

    <dt class="card-text">@Localizer["Email"]</dt>
    <dd class="card-text">@Model.Email</dd>
</dl>